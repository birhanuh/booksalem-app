{
  "compilerOptions": {
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
  },
  "exclude": [
    "node_modules"
  ],
}